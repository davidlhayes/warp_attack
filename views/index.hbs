  <div ng-controller="waCtrl">
    <article ng-repeat="message in messages">
      UserName: <h3> {[{message.user}]} </h3>
      Date: <h3> {[{message.date | date}]}
      Message: <h3> {[{ message.text }]} </h3>
      <img ng-src=" {[{ message.image }]} "><br>
      Likes: <h5> {[{message.likes}]} </h5>
      <p ng-click='plusLike($index)'><button type="button"
        name="button">Like</button></p>
      <p ng-click='minusLike($index)'><button type="button"
        name="button">Dislike</button></p>

      </p>
      <hr>
    </article>
  </div>
  

